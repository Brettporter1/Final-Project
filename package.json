{
    "name": "server",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "client": "cd client && npm start",
        "start": "node ./bin/www",
        "server": "nodemon --ignore 'client/'",
        "dev": "concurrently --kill-others-on-fail \" npm run server \" \"npm run client\""
    },
    "dependencies": {
        "concurrently": "^5.1.0",
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "express": "~4.16.1",
        "http-errors": "~1.6.3",
        "morgan": "~1.9.1",
        "node-sass-middleware": "0.11.0",
        "pug": "2.0.0-beta11"
    }
}
